{
  "name": "hire",
  "version": "0.0.1",
  "description": "testing choo",
  "main": "index.js",
  "scripts": {
    "deploy": "npm run compile && firebase deploy",
    "lint": "standard src/**/*.js",
    "lint:fix": "standard-format src/**/*.js -w",
    "start": "npm run copy && budo src/app.js --serve=dist/app.js --open -- -t babelify",
    "copy": "mkdir -p dist && cp src/index.html dist/",
    "compile": "npm run copy && browserify src/app.js -o dist/app.js -t babelify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mw222rs/choo-playground.git"
  },
  "author": "Mattias W",
  "license": "ISC",
  "dependencies": {
    "choo": "^2.2.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^8.3.0",
    "standard": "^7.1.2",
    "standard-format": "^2.2.1"
  }
}
